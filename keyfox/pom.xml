     <finalName>Base</finalName>  
       <plugins> 
          <plugin>  
               <groupId>org.apache.maven.plugins</groupId>  
               <artifactId>maven-war-plugin</artifactId>  
               <version>2.5</version>  
           </plugin>  
           <plugin>  
                <artifactId>maven-source-plugin</artifactId>  
                <version>2.1</version>  
                <configuration>  
                    <attach>true</attach>  
                </configuration>  
                <executions>  
                    <execution>  
                        <phase>compile</phase>  
                        <goals>  
                            <goal>jar</goal>  
                        </goals>  
                    </execution>  
                </executions>  
            </plugin>             
       </plugins> 
</build>

